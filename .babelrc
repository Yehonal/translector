{
    "presets": [
        ["@babel/preset-env", {
            "targets": {
                "node": "current"
            }
        }]
    ],
    "sourceMaps": "inline",
    "retainLines": true,
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        ["add-module-exports", {
            "addDefaultProperty": true
        }]
    ]
}