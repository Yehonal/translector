{
  "name": "translector",
  "productName": "Translector",
  "description": "A powerful utility for Translators",
  "version": "1.1.0",
  "private": true,
  "author": "hyperweb2",
  "homepage": ".",
  "license": "MIT",
  "main": "src/main/index.js",
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/register": "^7.9.0",
    "@hw-core/node-platform": "file:./src/deps/hw-core/node-platform",
    "compression": "^1.7.4",
    "electron-window-state": "^5.0.3",
    "express": "^4.17.1",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "graphql-tag": "^2.10.3",
    "module-alias": "^2.2.2",
    "node-fetch": "^2.6.0",
    "sequelize": "^5.21.7",
    "sequelize-graphql-schema": "file:./src/deps/sequelize-graphql-schema",
    "sqlite3": "^4.2.0",
    "tslint": "^6.1.2"
  },
  "scripts": {
    "start:electron": "electron .",
    "start:server": "node --preserve-symlinks ./src/server/boot.js",
    "start": "craco start --config cra-config.js",
    "build": "craco build --config cra-config.js",
    "build:ssg": "npm run build && npx react-snap",
    "build:electron": "electron-webpack",
    "test": "craco test --config cra-config.js",
    "eject": "react-scripts eject",
    "prestart": "node ./apps/installer/installer.js",
    "predeploy": "npm run build:ssg",
    "deploy": "gh-pages -d build",
    "dist:win64": "npm run package:win64 && electron-builder --win --x64 --prepackaged=dist/package/Translector-win32-x64",
    "package:win64": "electron-packager . Translector --overwrite --out=dist/package --platform=win32 --arch=x64 --icon=build/icon.ico --asar",
    "postinstall": "electron-rebuild"
  },
  "build": {
    "appId": "com.translector.app",
    "productName": "Translector",
    "asar": false,
    "win": {
      "icon": "build/icon.ico"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@craco/craco": "^5.6.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@hw-core/react-platform": "file:./src/deps/hw-core/react-platform",
    "@hw-core/reactstrap-modals": "file:./src/deps/hw-core/reactstrap-modals",
    "@material-ui/core": "^4.9.13",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "apollo-upload-client": "^13.0.0",
    "bootstrap": "^4.4.1",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "electron-packager": "^14.2.1",
    "electron-rebuild": "^1.10.1",
    "electron-settings": "^3.2.0",
    "electron-webpack": "^2.8.2",
    "font-awesome": "^4.7.0",
    "fontawesome": "^5.6.3",
    "gh-pages": "^2.2.0",
    "jquery": "^3.5.1",
    "pg-hstore": "^2.3.3",
    "popper.js": "^1.16.1",
    "prerender": "^5.6.0",
    "prerender-memory-cache": "^1.0.2",
    "prerender-node": "^3.2.5",
    "proxyquire": "^2.1.3",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-autobind": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-fade-in": "^1.0.0",
    "react-helmet": "^6.0.0",
    "react-markdown": "^4.3.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-sitemap": "^1.2.0",
    "react-scripts": "^3.4.1",
    "react-tinymce": "^0.7.0",
    "reactstrap": "^8.4.1",
    "tedious": "^8.3.0",
    "typescript": "^3.8.3"
  },
  "_moduleAliases": {
    "@root": ".",
    "@this/src": "src",
    "@this/conf": "src/conf",
    "@hw-core": "src/deps/hw-core"
  },
  "electronWebpack": {
    "main": {
      "webpackConfig": "webpack.config.js"
    }
  }
}
