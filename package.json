{
  "name": "translector",
  "productName": "Translector",
  "description": "A powerful utility for Translators",
  "version": "1.0.2",
  "private": true,
  "author": "hyperweb2",
  "homepage": ".",
  "license": "MIT",
  "main": "src/main/index.js",
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/register": "^7.0.0",
    "compression": "^1.7.3",
    "electron-window-state": "^5.0.3",
    "express": "^4.16.4",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "module-alias": "^2.2.0",
    "node-fetch": "^2.3.0",
    "sequelize": "^4.43.0",
    "sqlite3": "^4.0.6",
    "tslint": "^5.15.0",
    "@hw-core/node-platform": "file:./src/deps/hw-core/node-platform",
    "sequelize-graphql-schema": "file:./src/deps/sequelize-graphql-schema"
  },
  "scripts": {
    "start:electron": "electron .",
    "start:server": "node --preserve-symlinks ./src/server/boot.js",
    "start": "craco start --config cra-config.js",
    "build": "craco build --config cra-config.js",
    "build:ssg": "npm run build && npx react-snap",
    "build:electron": "electron-webpack",
    "test": "craco test --config cra-config.js",
    "eject": "react-scripts eject",
    "prestart": "node ./apps/installer/installer.js",
    "predeploy": "npm run build:ssg",
    "deploy": "gh-pages -d build",
    "dist:win64": "npm run package:win64 && electron-builder --win --x64 --prepackaged=dist/package/Translector-win32-x64",
    "package:win64": "electron-packager . Translector --overwrite --out=dist/package --platform=win32 --arch=x64 --icon=build/icon.ico --asar"
  },
  "build": {
    "appId": "com.translector.app",
    "productName": "Translector",
    "asar": false,
    "win": {
      "icon": "build/icon.ico"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@craco/craco": "^3.4.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@hw-core/react-platform": "file:./src/deps/hw-core/react-platform",
    "@hw-core/reactstrap-modals": "file:./src/deps/hw-core/reactstrap-modals",
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.4.12",
    "apollo-link-context": "^1.0.12",
    "apollo-link-http": "^1.5.9",
    "apollo-upload-client": "^10.0.0",
    "bootstrap": "^4.1.3",
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0",
    "electron-packager": "^13.1.1",
    "electron-webpack": "^2.6.2",
    "electron-rebuild": "^1.8.4",
    "font-awesome": "^4.7.0",
    "fontawesome": "^4.7.2",
    "gh-pages": "^2.0.1",
    "jquery": "^3.3.1",
    "pg-hstore": "^2.3.2",
    "popper.js": "^1.14.6",
    "prerender": "^5.5.1",
    "prerender-memory-cache": "^1.0.2",
    "prerender-node": "^3.2.1",
    "proxyquire": "^2.1.0",
    "react": "^16.7.0",
    "react-apollo": "^2.4.0",
    "react-autobind": "^1.0.6",
    "react-dom": "^16.7.0",
    "react-fade-in": "^0.1.6",
    "react-helmet": "^5.2.0",
    "react-markdown": "^4.0.6",
    "react-router-dom": "^4.3.1",
    "react-router-sitemap": "^1.2.0",
    "react-scripts": "^2.1.8",
    "react-tinymce": "^0.7.0",
    "reactstrap": "^6.5.0",
    "tedious": "^6.1.0",
    "typescript": "^3.3.3"
  },
  "_moduleAliases": {
    "@root": ".",
    "@this/src": "src",
    "@this/conf": "src/conf",
    "@hw-core": "src/deps/hw-core"
  },
  "electronWebpack": {
    "main": {
      "webpackConfig": "webpack.config.js"
    }
  }
}